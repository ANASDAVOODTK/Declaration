<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_margin="5dp"
    app:cardCornerRadius="10sp">
    <LinearLayout
        android:padding="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/cname"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:textColor="#470DAF"
            android:text="Center Name : phc kavanur" />

        <TextView
            android:id="@+id/cid"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#094E2A"
            android:text="Cnter Id : 123422"/>
        <TextView
            android:id="@+id/address"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:textColor="#094E2A"
            android:layout_height="wrap_content"
            android:text="address: areepurath house kavnur "/>

        <TextView
            android:id="@+id/blockname"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#094E2A"
            android:text="Center Name : phc kavanur" />

        <TextView
            android:id="@+id/pincode"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#094E2A"
            android:text="Cnter Id : 123422"/>
        <TextView
            android:id="@+id/time"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:textColor="#094E2A"
            android:layout_height="wrap_content"
            android:text="address: areepurath house kavnur "/>
        <TextView
            android:id="@+id/feetype"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:textColor="#094E2A"
            android:layout_height="wrap_content"
            android:text="address: areepurath house kavnur "/>

        <TextView
            android:id="@+id/avc"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#094E2A"
            android:text="Cnter Id : 123422"/>
        <TextView
            android:id="@+id/avcpd1"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:textColor="#094E2A"
            android:layout_height="wrap_content"
            android:text="address: areepurath house kavnur "/>

        <TextView
            android:id="@+id/avcpd2"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#094E2A"
            android:text="Cnter Id : 123422"/>
        <TextView
            android:id="@+id/agelimit"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:textColor="#094E2A"
            android:layout_height="wrap_content"
            android:text="address: areepurath house kavnur "/>
        <TextView
            android:id="@+id/vaccinetype"
            android:fontFamily="@font/montserrat_bold"
            android:layout_width="match_parent"
            android:textColor="#094E2A"
            android:layout_height="wrap_content"
            android:text="address: areepurath house kavnur "/>
    </LinearLayout>
</androidx.cardview.widget.CardView>